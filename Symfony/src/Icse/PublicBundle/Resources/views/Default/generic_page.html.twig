{% extends (freshers is defined and freshers) ? 'IcsePublicBundle:SignUp:freshers_template.html.twig' : 'IcsePublicBundle:Default:template.html.twig' %} 
{% set currentPage = pageId %}

{% block title %} - {{ pageTitle }}{% endblock %} 

{% block content %}

{% autoescape false %} 
{{ pageBody }}
{% endautoescape %} 

{% if reloadPeriod is defined %}
<script>
$(document).ready(function(){ 
  setTimeout(function(){window.top.location.reload(true);}, {{ reloadPeriod }});
});
</script>
{% endif %}

{% endblock %}
