<table class=admintable>
  <tr>
    {% if row_prefixes is defined %}
    <th></th>
    {% endif %}
    {% for column in columns %}
    <th>{{ column.heading }}</th>
    {% endfor %}
  </tr>
  
  {% for entity in entities %}
  <tr data-entity="{{ entity | serialize }}" class="{{ cycle(['odd', 'even'], loop.index) }}">
    {% if row_prefixes is defined %}
    <td>{{ rowPrefixes }}</td>
    {% endif %}
    {% for column in columns %}
    <td>{{ entity | getCell(column) }}</td>
    {% endfor %}
  </tr>
  {% endfor %}
</table>
